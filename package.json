    "type": "git",
    "url": "https://github.com/makerdao/multicall.js.git"
  },
  "main": "dist/build.cjs.js",
  "module": "dist/build.esm.js",
  "browser": "dist/build.esm.js",
  "browserslist": "> 0.25%, not dead",
  "files": [
    "src/*",
    "test/*",
    "examples/*",
    "dist/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && rollup -c",
    "watch": "yarn clean && rollup -w -c",
    "example": "DEBUG=multicall node_modules/.bin/babel-node examples/es-example.js",
    "test": "jest --runInBand",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "prepublishOnly": "yarn test && yarn build"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "debug": "^4.1.1",
